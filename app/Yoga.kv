<Session>
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: "materials\kivy_wallpaper.jpg"
    
   
    BoxLayout:
        orientation: 'vertical'
        padding: 10
        size_hint: (1, 1)
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
        
        
        
        
        BoxLayout:
            orientation: 'horizontal'
            size_hint_y: 0.9
            pos_hint: {'center_x': 0.5, 'center_y': 0.9}
            

            BoxLayout:
                orientation: 'vertical'
                size_hint_y: 0.9
                size_hint_x: 0.3
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                canvas.before:
                    Color:
                        rgba: 0.43, 0.35, 0, 0.4  # Set the background color (RGBA values)
                    Rectangle:
                        pos: self.pos
                        size: self.size
                

                Image:
                    source: "materials/Yogaposes/Posewindow.png"
                    allow_stretch: True
                    id: pose_frame
                    size_hint_x: 1
                    size_hint_y: 0.6
                    pos_hint:{'center_x': 0.5, 'center_y': 0.95}

                BoxLayout:
                    orientation: 'vertical'
                    size_hint_y: 0.2
                    pos_hint: {'center_x': 0.52, 'center_y': 0.4}
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, 0  # Set the background color (RGBA values)
                        Rectangle:
                            pos: self.pos
                            size: self.size

                    Label
                        text:"Predicted Pose: "
                        font_style: "H6"
                        halign: 'center'
                        font_name: "font/zh-cn.ttf"
                        pos_hint:{'x': 0.5, 'top': 0.4}
                        size_hint: 0.1, 0.1
                        font_size: '25sp'
                
                    Label
                        id: prediction_label
                        font_style: "H6"
                        halign: 'center'
                        font_name: "font/zh-cn.ttf"
                        pos_hint:{'x': 0.5, 'top': 0.4}
                        size_hint: 0.1, 0.1
                        font_size: '50sp'

                BoxLayout:
                    orientation: 'horizontal'
                    size_hint_y: 0.2
                    pos_hint: {'center_x': 0.53, 'center_y': 0.4}
                    

                    Label
                        text: "Pose Probability: "
                        font_style: "H6"
                        halign: 'center'
                        font_name: "font/zh-cn.ttf"
                        pos_hint:{'x': 0.8, 'top': 0.4}
                        size_hint: 0.1, 0.1
                        font_size: '25sp'

                    Label
                        id: probability_label
                        font_style: "H6"
                        halign: 'center'
                        font_name: "font/zh-cn.ttf"
                        pos_hint:{'x': 0.8, 'top': 0.4}
                        size_hint: 0.1, 0.1
                        font_size: '25sp'
            
            BoxLayout:
                orientation: 'vertical'
                spacing: dp(10)
                padding: 20
                size_hint_y: 0.9
                size_hint_x: 0.6
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                canvas.before:
                    Color:
                        rgba: 0.43, 0.35, 0, 0.4  # Set the background color (RGBA values)
                    Rectangle:
                        pos: self.pos
                        size: self.size
                

                
                
                BoxLayout:
                    orientation: 'horizontal'
                    size_hint_y: 0.1
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}
                    
                    Label:
                        text: "Timer"
                        halign: 'center'
                        font_name: "font/zh-cn.ttf"
                        pos_hint: {'x': 0.5,'top': 0.5}
                        size_hint: 0.1, 0.1
                        font_size: '40sp'
                
                    Label:
                        id: timer_label
                        halign: 'center'
                        font_name: "font/zh-cn.ttf"
                        pos_hint: {'x': 0.5,'top': 0.5}
                        size_hint: 0.1, 0.1
                        font_size: '60sp'

                BoxLayout:
                    orientation: 'vertical'
                    size_hint_y: 0.7
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}
                    
               

                    Image:
                        source: "materials/Yogaposes/Camerascreen.png"
                        allow_stretch: True
                        id: camera_frame
                        size_hint: 1, 1
                        pos_hint:{'center_x': 0.5, 'center_y': 0.5}
                        
                    Label
                        text:"Calibration Phase"
                        id: calibration_state
                        halign: 'center'
                        font_style: "H6"
                        font_name: "font/zh-cn.ttf"
                        pos_hint:{'x': 0.46, 'top': 0.5}
                        size_hint: 0.1, 0.1
                        font_size: '25sp'

                    

                BoxLayout:
                    orientation: 'horizontal'
                    size_hint_y: 0.2
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}

                    
                    

                    Label
                        text:"Current Pose: "
                        halign: 'center'
                        font_style: "H6"
                        font_name: "font/zh-cn.ttf"
                        pos_hint:{'x': 0.4, 'top': 0.5}
                        size_hint: 0.1, 0.1
                        font_size: '30sp'

                    Label
                        id: pose_label
                        halign: 'center'
                        font_style: "H6"
                        font_name: "font/zh-cn.ttf"
                        pos_hint:{'x': 0.4, 'top': 0.5}
                        size_hint: 0.1, 0.1
                        font_size: '50sp'

                   
                
                    

        BoxLayout:
            orientation: 'horizontal'
            spacing: dp(10)
            size_hint_y: 0.1
            pos_hint: {'center_x': 0.5, 'center_y': 0.2}
            
            MDRaisedButton:
                style: "filled"
                id: session_start_button
                text: "Start"
                size_hint: (1, 0.2)
                font_style: "H6"
                font_name: "font/zh-cn.ttf"
                md_bg_color: app.get_hex_color("#FFBD00A6")
                size_hint: (0.1, 0.7)
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}

                on_press: root.activation_trigger()
            
            
               
            MDRaisedButton:
                style: "filled"
                text: "Go back"
                font_style: "H6"
                font_name: "font/zh-cn.ttf"
                md_bg_color: app.get_hex_color("#FFBD00A6")
                size_hint: (0.1, 0.7)
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}

                on_press: 
                    root.manager.transition = FadeTransition()
                    root.manager.current = 'session-library'


