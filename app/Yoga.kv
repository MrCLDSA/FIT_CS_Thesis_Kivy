<MainMenu>:
    

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: "materials\kivy_wallpaper.jpg"

    BoxLayout:
        orientation: 'vertical'
        spacing: dp(10)
        size_hint: (1,1)
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}  
        

        BoxLayout:
            orientation: 'vertical'
            size_hint: (0.9, 0.1)
            pos_hint: {'center_x': 0.5, 'center_y': 0.3}
            
            MDLabel:
                text: "Yoga"
                theme_text_color: "Custom"
                text_color: "white"
                font_style: "H4"
                font_name: "font/zh-cn.ttf"
                halign: 'center'
                size_hint: (1, 0.1)
                pos_hint: {'center_x': 0.5, 'center_y': .90}  
        

            MDLabel:
                text: "Main Menu"
                theme_text_color: "Custom"
                text_color: "white"
                font_style: "H6"
                font_name: "font/zh-cn.ttf"
                halign: 'center'
                size_hint: (1, 0.1)
                pos_hint: {'center_x': 0.5, 'center_y': 0.95} 
        
        BoxLayout:
            orientation: 'horizontal'
            spacing: dp(10)
            size_hint: (0.9, 0.9)
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
            
        
            BoxLayout:
                orientation: 'vertical'
                spacing: dp(10)
                size_hint: (0.5, 0.5)
                pos_hint: {'center_x': 1, 'center_y': 0.5}
                

                MDRaisedButton:
                    text: "Go to Session"
                    font_style: "H6"
                    font_name: "font/zh-cn.ttf"
                    md_bg_color: app.get_hex_color("#FFBD00A6")
                    size_hint: (0.8, 1.2)
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}
           
                    on_press: 
                        root.next_menu()

                MDRaisedButton:
                    style: "filled"
                    text: "Pose Library"
                    font_style: "H6"
                    font_name: "font/zh-cn.ttf"
                    md_bg_color: app.get_hex_color("#FFBD00A6")
                    size_hint: (0.8, 1.2)
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}
            
                    on_press: 
                        root.manager.transition = FadeTransition()
                        root.manager.current = 'pose-library'
            
                MDRaisedButton:
                    style: "filled"
                    text: "Settings"
                    font_style: "H6"
                    font_name: "font/zh-cn.ttf"
                    md_bg_color: app.get_hex_color("#FFBD00A6")
                    size_hint: (0.8, 1.2)
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}

                    on_press: 
                        root.manager.transition = FadeTransition()
                        root.manager.current = 'settings'

                MDRaisedButton:
                    style: "filled"
                    text: "About"
                    font_style: "H6"
                    font_name: "font/zh-cn.ttf"
                    md_bg_color: app.get_hex_color("#FFBD00A6")
                    size_hint: (0.8, 1.2)
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}

                    on_press: 
                        root.manager.transition = FadeTransition()
                        root.manager.current = 'about'

                MDRaisedButton:
                    style: "filled"
                    text: "Quit"
                    font_style: "H6"
                    font_name: "font/zh-cn.ttf"
                    md_bg_color: app.get_hex_color("#FFBD00A6")
                    size_hint: (0.8, 1.2)
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}

                    on_press: 
                        root.quit_app()



            BoxLayout:
                orientation: 'vertical'
                spacing: dp(10)
                size_hint: (0.5, 0.9)
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                

        
                CustomImageCarousel2:
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5} 
        
        



